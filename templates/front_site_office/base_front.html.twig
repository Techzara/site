{% set current_route = app.request.attributes.get('_route') %}

<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,600,700,800&amp;subset=cyrillic-ext,latin-ext" rel="stylesheet">
            <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" crossorigin="anonymous">
            <link rel="stylesheet" href="{{ asset('front_site_office/tznew/css/animate.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/tznew/css/icomoon.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/tznew/css/bootstrap.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/tznew/css/superfish.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/tznew/css/style.css') }}">
        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('front_site_office/images/techzara/icon.jpg') }}"/>
    </head>

    <body id="{% if current_route == 'home_site_index' %}id-home{% endif %}" class="tz-front-site">
        <div id="tz-wrapper">
            <div id="tz-page">
            {{ include("front_site_office/base_front_header.html.twig") }}
                <!-- Main content -->
                {% block body %}{% endblock %}

            {{ render(controller('FrontSiteBundle:TzFooter:show')) }}
            </div>
        </div>

        {% block javascripts %}
            <script>
                window.FontAwesomeConfig = {
                    searchPseudoElements: true
                }
            </script>
            {#techzara new#}
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/jquery.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/bootstrap.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/jquery.easing.1.3.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/jquery.waypoints.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/sticky.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/jquery.stellar.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/hoverIntent.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/superfish.js') }}"></script>
            <script type="text/javascript" src="{{ asset('front_site_office/tznew/js/main.js') }}"></script>
        {% endblock %}
    </body>
</html>